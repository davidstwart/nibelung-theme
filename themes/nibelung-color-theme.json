{
  "name": "Nibelung",
  "type": "light",
  "semanticHighlighting": true,
  "tokenColors": [
    {
			"scope": [
			        	"meta.embedded",
			        	"source.groovy.embedded"
			        ],
			"settings": {
				"foreground": "#212529"
			}
		},
		{
			"scope": "emphasis",
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"scope": "strong",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"scope": [
                "comment",
                "punctuation.definition.comment"
              ],
			"settings": {
				"foreground": "#adb5bd"
			}
		},
		{
			"scope": "constant.language",
			"settings": {
				"foreground": "#6c757d"
			}
		},

    {
        "scope": "entity.name.tag",
        "settings": {
            "foreground": "#6c757d"
        }
    },
		{
			"scope": "invalid",
			"settings": {
				"foreground": "#f44747"
			}
		},
		{
			"scope": "markup.underline",
			"settings": {
				"fontStyle": "underline"
			}
		},
		{
			"scope": "markup.bold",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"scope": "markup.heading",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#212529"
			}
		},
		{
			"scope": "markup.italic",
			"settings": {
				"fontStyle": "italic"
			}
		},
    {
			"scope": [
                "storage",
                "storage.type",
                "storage.modifier"
              ],
			"settings": {
				"foreground": "#6c757d"
			}
		},
		{
			"scope": [
                "string",
                "string.tag",
                "string.value"
              ],
			"settings": {
				"foreground": "#212529"
			}
		},
		{
			"scope": [
                "string.html",
                "string.quoted.double.html",
                "string.quoted.single.html",
                "string.quoted.double.js.jsx",
                "entity.other.attribute-name.html",
                "entity.other.attribute-name.js.jsx",
                "meta.tag.attributes.js.jsx"
              ],
			"settings": {
				"foreground": "#212529"
			}
		},
    {
			"name": "brackets of XML/HTML tags",
			"scope": [
                "punctuation.definition.tag",
                "punctuation.definition.string.begin.html",
                "punctuation.definition.string.end.html",
                "punctuation.definition.string.begin.js.jsx",
                "punctuation.definition.string.end.js.jsx",
                "keyword.operator.assignment.js.jsx"
              ],
			"settings": {
				"foreground": "#adb5bd"
			}
		},
    {
      "scope": "meta.jsx.children.js.jsx",
      "settings": {
          "foreground": "#212529"
      }
    },
    {
      "scope": [
                "entity.name.tag.css",
                "entity.other.attribute-name.class.css",
                "entity.other.attribute-name.class.mixin.css",
                "entity.other.attribute-name.id.css",
                "entity.other.attribute-name.parent-selector.css",
                "entity.other.attribute-name.pseudo-class.css",
                "entity.other.attribute-name.pseudo-element.css",
                "source.css.less entity.other.attribute-name.id",
                "entity.other.attribute-name.attribute.scss",
                "entity.other.attribute-name.scss"
              ],
      "settings": {
        "foreground": "#212529"
      }
    },
		{
			"scope": [
			        	"support.type.vendored.property-name",
			        	"support.type.property-name",
			        	"variable.css",
			        	"variable.scss",
			        	"variable.other.less",
			        	"source.coffee.embedded"
			        ],
			"settings": {
				"foreground": "#212529"
			}
		},
 		{
			"scope": [
                "constant.numeric.css",
		        		"support.constant.property-value",
		        		"support.constant.font-name",
		        		"support.constant.media-type",
		        		"support.constant.media",
		        		"constant.other.color.rgb-value",
		        		"constant.other.rgb-value",
		        		"support.constant.color"
		        	],
			"settings": {
				"foreground": "#6c757d"
			}
		},
  	{
			"scope": "keyword.other.unit",
			"settings": {
				"foreground": "#adb5bd"
			}
		},
		{
			"scope": [
                "keyword",
                "keyword.control",
                "keyword.operator.expression",
			        	"keyword.operator.new",
			        	"keyword.operator.sizeof",
			        	"keyword.operator.instanceof",
                "keyword.operator.cast",
                "support.type.primitive.ts",
                "variable.language"                
              ],
			"settings": {
				"foreground": "#6c757d"
			}
		},
		{
			"scope": [
                "constant.numeric",
                "constant.language.boolean",
                "constant.language.null",
                "constant.language.undefined",
                "constant.language.nan",
                "constant.language.infinity"
              ],
			"settings": {
				"foreground": "#212529"
			}
		},
		{
			"scope": [
                "punctuation",
                "meta.brace.square.js",
                "meta.brace.round.js",
                "meta.brace.square.ts",
                "meta.brace.round.ts",
                "meta.var.expr.js.jsx"
			        ],
			"settings": {
				"foreground": "#adb5bd"
			}
		},
        {
			"scope": [
                "keyword.operator",
                "keyword.operator.logical",
                "storage.type.function.arrow.js",
                "meta.arrow.js"
              ],
			"settings": {
				"foreground": "#adb5bd"
			}
		},
    {
			"scope": [
				        "entity.name.function",
				        "support.function",
				        "support.constant.handlebars",
                "meta.method.declaration.js",
                "meta.method.declaration.js storage.type.js"
			        ],
			"settings": {
				"foreground": "#212529"
			}
		},
		{
			"scope": [
		        		"meta.return-type",
		        		"support.class",
		        		"support.type",
		        		"entity.name.type",
		        		"entity.name.class"
		        	],
			"settings": {
				"foreground": "#212529"
			}
		},
		{
			"scope": [
			        	"meta.type.cast.expr",
			        	"meta.type.new.expr",
			        	"support.constant.math",
			        	"support.constant.dom",
			        	"support.constant.json",
			        	"entity.other.inherited-class"
			        ],
			"settings": {
				"foreground": "#212529"
			}
		},
		{
			"scope": [
		        		"variable",
		        		"meta.definition.variable.name",
		        		"support.variable",
		        		"entity.name.variable",
                "meta.object-literal.key"
		        	],
			"settings": {
				"foreground": "#212529"
			}
		}
    ],
  "colors": {
    "editorLink.activeForeground": "#008cff",
    "foreground": "#343a40",
    "focusBorder": "#00000000",
    "selection.background": "#343a403c",

    "activityBar.foreground": "#343a40",
    "activityBar.background": "#f8f9fa",
    "activityBar.inactiveForeground": "#ced4da",
    "activityBarBadge.foreground": "#e9ecef",
    "activityBarBadge.background": "#6c757d",
    
    "sideBar.background": "#f8f9fa",
    "sideBar.foreground": "#6c757d",
    "sideBarSectionHeader.background": "#00000000",
    "sideBarSectionHeader.border": "#00000000",
    
    "quickInput.background": "#e9ecef",
    "quickInput.foreground": "#6c757d",
    "quickInputList.focusBackground": "#e9ecef",
    "quickInputList.focusForeground": "#212529",

    "list.inactiveSelectionBackground": "#e9ecef",
    "list.inactiveSelectionForeground": "#6c757d",
    "list.hoverBackground": "#e9ecef",
    "list.hoverForeground": "#343a40",
    "list.activeSelectionBackground": "#e9ecef",
    "list.activeSelectionForeground": "#6c757d",
    "list.focusBackground": "#e9ecef",
    "list.focusForeground": "#343a40",
    "tree.indentGuidesStroke": "#00000000",
    "list.dropBackground": "#343a403c",
    
    "statusBar.foreground": "#343a40",
    "statusBar.background": "#e9ecef",
    "statusBar.debuggingForeground": "#343a40",
    "statusBar.debuggingBackground": "#e9ecef",
    "statusBar.noFolderForeground": "#343a40",
    "statusBar.noFolderBackground": "#e9ecef",
    
    "titleBar.activeBackground": "#f8f9fa",
    "titleBar.inactiveBackground": "#f8f9fa",
    "titleBar.border": "#00000000",

    "menu.foreground": "#6c757d",
    "menu.background": "#e9ecef",
    "menu.selectionForeground": "#212529",
    "menu.selectionBackground": "#e9ecef",
    "menu.separatorBackground": "#00000000",

    "editor.background": "#f8f9fa",
    "editor.foreground": "#343a40",
    "editorCursor.foreground": "#000000ba",
    "editor.selectionBackground": "#ced4daa4",
    "editorWhitespace.foreground": "#6c757d3c",
    "editor.selectionHighlightBackground": "#00000000",
    "editor.selectionHighlightBorder": "#00000000",
    
    "editor.findMatchBackground": "#95999ea4",
    "editor.findMatchBorder": "#00000000",
    "editor.findMatchHighlightBackground": "#ced4daa4",
    "editor.findMatchHighlightBorder": "#00000000",
    "editor.findRangeHighlightBackground": "#adb5bd3c",
    "editor.findRangeHighlightBorder": "#00000000",
    "editor.rangeHighlightBackground": "#00000000",
    "editor.rangeHighlightBorder": "#00000000",
    
    "editor.wordHighlightBackground": "#00000000",
    "editor.lineHighlightBackground": "#00000000",
    "editor.lineHighlightBorder": "#00000000",
    "editorIndentGuide.background1": "#f8f9fa",
    "editorIndentGuide.activeBackground1": "#6c757d",
    "editorLineNumber.foreground": "#ced4da",
    "editorLineNumber.activeForeground": "#6c757d",
    
    "editorBracketMatch.background": "#00000000",
    "editorBracketMatch.border": "#00000000",
    "editorCodeLens.foreground": "#ced4da",
    "editorGroup.border": "#00000000",
    
    "panel.background": "#f8f9fa",
    "panel.border": "#00000000",
    "terminal.foreground": "#ced4da",
       
    "editorGroupHeader.tabsBackground": "#f8f9fa",
    "tab.activeBackground": "#e9ecef",
    "tab.inactiveBackground": "#f8f9fa",
    "tab.inactiveForeground": "#6c757d",
    "breadcrumb.foreground": "#adb5bd",
    "breadcrumb.focusForeground": "#343a40",

    "scrollbarSlider.background": "#6c757d9c",
  }
}
